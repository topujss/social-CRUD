<div class="profile-body">
  <div class="profile-wraper">
    <div class="profile-info">
      <%- include("./layouts/info") %>
      <hr />
      <%- include("./layouts/menu") %>
    </div>
    <div class="profile-box">
      <div class="box-content">
        <h2>Change gallery Photo</h2>
        <hr />
        <% if (message) { %>
        <p class="text-red-500"><%= message %></p>
        <% } %>
        <div class="form-wraper">
          <form action="/gallery-up" method="post" enctype="multipart/form-data">
            <div class="form-group">
              <label for="gallery_photo">Select multiple Gallery Photo</label>
              <div class="profile-photo gallery"></div>
              <label class="upload border-t-2 border-indigo-600 pt-4">
                <i class="text-rose-700 fa fa-photo uplaod"></i>
                <input
                  id="gallery_photo"
                  multiple
                  name="gallery-p"
                  style="display: none"
                  type="file"
                />
              </label>
            </div>
            <div class="form-group">
              <button>Update gallery</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
