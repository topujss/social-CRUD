<div class="profile-body">
  <div class="profile-wraper">
    <div class="profile-info">
      <%- include("./layouts/info") %>
      <hr />
      <%- include("./layouts/menu") %>
    </div>
    <div class="profile-box">
      <div class="box-content">
        <h2>user profile information update</h2>
        <hr />
        <div class="form-wraper">
          <form action="">
            <div class="form-group">
              <label for="name">Name</label>
              <input id="name" type="text" />
            </div>
            <div class="form-group">
              <label for="email">Email address</label>
              <input id="email" type="email" />
            </div>
            <div class="form-group">
              <label for="userme">Username</label>
              <input id="userme" type="text" />
            </div>
            <div class="form-group">
              <label for="locate">Location</label>
              <input id="locate" type="text" />
            </div>
            <div class="form-group">
              <label for="ph">Phone number</label>
              <input id="ph" type="text" />
            </div>
            <div class="form-group">
              <label for="ge">Gender</label>
              <input id="ge" type="text" />
            </div>
            <div class="form-group">
              <button>Update</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
